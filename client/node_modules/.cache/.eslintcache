         <Form.Label>Enter New Password</Form.Label>\n                    <Form.Control required type=\"password\" value={new1} onChange={(e)=>setNew1(e.target.value)}/>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Enter New Password</Form.Label>\n                    <Form.Control required type=\"password\" value={new2} onChange={(e)=>setNew2(e.target.value)}/>\n                </Form.Group>\n                {pop && <h6>Password does not match</h6>}\n                {msg.length>0 && <h5>{msg}</h5>}\n                <Button variant=\"primary\" type=\"submit\">\n                    Change Password\n                </Button>\n            </Form>\n            </Jumbotron>\n        </div>\n    )\n}\n\nexport default ChangePassword\n","/Users/abhishekswaraj/Desktop/Alumni-System/client/src/components/Reset.js",["223","224","225"],"import React,{useState,useEffect} from 'react'\nimport axios from 'axios'\nimport { Button, Form, Jumbotron } from 'react-bootstrap';\n\nfunction Reset(props) {\n    const token=props.match.params.token;\n    const [new1, setNew1] = useState('')\n    const [new2, setNew2] = useState('')\n    const [msg, setMsg] = useState('');\n    const submithandler=(e)=>{\n        e.preventDefault();\n        if(new1==new2)\n        {\n            axios.post(`/users/resetpass`,{token,pass:new1})\n            .then((data)=>{\n                setMsg(data.data.msg)\n                setInterval(()=>{\n                    props.history.push('/');\n                },2000)\n            })\n            .catch((err)=>console.log(err))\n        }\n    }\n    return (\n        <div style={{width:400,height:50,marginLeft:500,marginTop:200}}>\n            <Jumbotron>\n                <h1>Reset Password</h1>\n            <Form onSubmit={submithandler}>\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Enter New Password</Form.Label>\n                    <Form.Control type=\"password\" value={new1} onChange={(e)=>setNew1(e.target.value)}/>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Enter New Password</Form.Label>\n                    <Form.Control type=\"password\" value={new2} onChange={(e)=>setNew2(e.target.value)}/>\n                </Form.Group>\n                {msg.length>0 && <h5>{msg}</h5>}\n                <button  type=\"submit\" className=\"btn btn-primary btn-block\">\n                    Reset Password\n                </button>\n            </Form>\n            </Jumbotron>\n        </div>\n    )\n}\n\nexport default Reset\n","/Users/abhishekswaraj/Desktop/Alumni-System/client/src/components/ResetPass.js",[],"/Users/abhishekswaraj/Desktop/Alumni-System/client/src/components/SignupAdmin.js",[],"/Users/abhishekswaraj/Desktop/Alumni-System/client/src/components/UpdateByAdmin.js",["226","227","228"],"import React, {useState,useEffect} from 'react'\nimport Form from 'react-bootstrap/Form'\nimport axios from 'axios'\nimport Button from 'react-bootstrap/Button'\nimport Alert from 'react-bootstrap/Alert'\nfunction Update(props) {\n    const id=props.match.params.id;\n    const [user, setUser] = useState({\n        name:'',roll:0,branch:'',mail:'',mob:0,year:0,pass:'',designation:'',organization:''\n    })\n    const [success, setSuccess] = useState(false)\n    const [fail, setFail] = useState(false)\n    const [subm, setSubm] = useState(false)\n    useEffect(() => {\n        async function fetchAPI()\n        {\n            await axios.get('/users/'+id)\n            .then((data)=>\n            {\n                user=setUser(data.data[0]);\n                console.log(user.name);\n            })\n            .catch((err)=>console.log(\"oh\"+err))\n        }\n        fetchAPI();\n    }, [])\n    useEffect(() => {\n        setFail(false);\n        setSuccess(false);\n        setSubm(true);\n    }, [user])\n    const osubmit=(event) => {\n        event.preventDefault();\n        axios.put('/users/'+id+'/edit',user)\n        .then((data)=>{\n            if(!data.data.code){\n                setSuccess(true);\n                console.log(data)\n            }\n            else\n            {\n\n                setFail(true);\n            }\n            window.scrollTo({\n                  top: 0,\n                  behavior: \"smooth\"\n                });\n        })\n        .catch((err)=>console.log(err))\n    }\n    return (\n        <div className='container' style={{width:500,height:50,marginLeft:450,marginTop:20}}>\n            <h1>Update your Profile</h1>\n            {success && \n            <Alert variant=\"success\">\n                <Alert.Heading>Updated Successfully. </Alert.Heading>\n            </Alert>}\n            {fail && \n                <Alert variant=\"danger\">\n                    <Alert.Heading>Invalid Input Unable to Update</Alert.Heading>\n                </Alert>\n            }\n            <Form onSubmit={osubmit}>\n                <Form.Group controlId=\"formBasicEmail\">\n                    <Form.Label>Name</Form.Label>\n                    <Form.Control type=\"name\" value={user.name} onChange={(e)=>setUser({...user,name:e.target.value})}/>\n                </Form.Group>\n\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Roll No.</Form.Label>\n                    <Form.Control type=\"name\" value={user.roll} onChange={(e)=>setUser({...user,roll:e.target.value})}/>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Year of Graduation</Form.Label>\n                    <Form.Control type=\"number\" value={user.year} onChange={(e)=>setUser({...user,year:e.target.value})}/>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Email</Form.Label>\n                    <Form.Control type=\"mail\" value={user.mail} onChange={(e)=>setUser({...user,mail:e.target.value})}/>\n    \n                </Form.Group>\n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>Mobile No.</Form.Label>\n                    <Form.Control type=\"tel\" value={user.mob} onChange={(e)=>setUser({...user,mob:e.target.value})}/>\n                </Form.Group>\n                <Form.Group controlId=\"exampleForm.ControlSelect1\">\n                    <Form.Label>Branch</Form.Label>\n                    <Form.Control as=\"select\" value={user.branch} onChange={(e)=>setUser({...user,branch:e.target.value})}>\n                    <option>CSE</option>\n                    <option>CE</option>\n                    <option>ME</option>\n                    <option>ECE</option>\n                    <option>EE</option>\n                    </Form.Control>\n                </Form.Group>\n                <Button variant=\"primary\" className=\"btn btn-primary btn-block\" type=\"submit\">\n                    Update\n                </Button>\n            </Form>\n            \n        </div>\n    )\n}\n\nexport default Update\n","/Users/abhishekswaraj/Desktop/Alumni-System/client/src/components/LoginAdmin.js",[],"/Users/abhishekswaraj/Desktop/Alumni-System/client/src/components/Profile.js",["229"],"import React from 'react'\nimport {Link} from 'react-router-dom'\nimport axios from 'axios'\nimport Button from 'react-bootstrap/Button'\nfunction Profile(props) {\n    const users=props.users;\n    const clickhandler=(roll)=>{\n        if(window.confirm('Are you sure you want to delete '+roll))\n        {   \n            axios.delete(`http://localhost:8080/admin/delete/${roll}`)\n            .then((data)=>{\n                if(data.status==200)\n                {\n                    props.fetch();\n                }\n            })\n            .catch((err)=>console.log(err))\n        }\n    }\n    return (\n        <div>\n            {us